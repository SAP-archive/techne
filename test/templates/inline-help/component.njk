<!--
inline-help:
    properties={},
    modifier={ block: [] }
-->
{% macro inline_help(properties={}, modifier={}, state={}, aria={}) -%}
<span class="tn-inline-help"{{ aria | aria }}>
    <span class="tn-inline-help__content{{ modifier.content | modifier('inline-help__content') }}">
            {{ properties.content }}
    </span>
</span>
{%- endmacro %}
